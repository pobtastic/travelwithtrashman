; Start with comparing against 1UP.
 $85D1 LD DE,$8C28   ; #REGde=#R$8C28.
 $85D4 AND A         ; {If the currently active player is not 1UP then jump to #R$85DC
 $85D5 SBC HL,DE     ; (which will then set it to 1UP).
 $85D7 JR NZ,$85DC   ; }
